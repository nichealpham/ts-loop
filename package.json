{
	"name": "microservice",
	"version": "2.1.0",
	"description": "Backend design pattern for microservices using loopback 3.0 and typescript",
	"scripts": {
		"override": "yes | cp -Rf libs/* node_modules",
		"build": "./node_modules/.bin/tsc && ./node_modules/.bin/grunt copy",
		"setup": "npm i && npm run override",
		"dev": "./node_modules/.bin/nodemon .",
		"start": "npm run build && node dest/server.js",
		"docker": "chmod +x ./shells/gcp/gcp.docker.deploy.sh && ./shells/gcp/gcp.docker.deploy.sh",
		"commit": "git push origin new-struct && git checkout master && git merge new-struct && git push origin master && git checkout new-struct"
	},
	"dependencies": {
		"@google-cloud/storage": "^2.4.0",
		"app-root-path": "^2.1.0",
		"axios": "^0.18.0",
		"compression": "^1.7.3",
		"cors": "^2.8.5",
		"cron": "^1.6.0",
		"crypto": "^1.0.1",
		"express-useragent": "^1.0.12",
		"loopback": "^3.24.2",
		"loopback-boot": "^2.27.1",
		"loopback-connector-mongodb": "^3.9.2",
		"loopback-connector-sendgrid": "^2.2.4",
		"loopback-softdelete-mixin4": "^1.0.8",
		"loopback-swagger": "^5.7.2",
		"moment": "^2.24.0",
		"paypal-rest-sdk": "^1.8.1",
		"rimraf": "^2.6.3",
		"strong-error-handler": "^3.2.0",
		"swagger-client": "^3.8.22",
		"swagger-ui-express": "^4.0.2",
		"validator": "^10.10.0",
		"zlib": "^1.0.5"
	},
	"devDependencies": {
		"@types/app-root-path": "^1.2.4",
		"@types/loopback": "^3.2.6",
		"@types/loopback-boot": "^2.23.4",
		"@types/node": "^10.9.4",
		"@types/request-promise": "^4.1.42",
		"@types/rimraf": "^2.0.2",
		"eslint": "^5.12.1",
		"grunt": "^1.3.0",
		"grunt-contrib-copy": "^1.0.0",
		"nodemon": "^1.18.9",
		"ts-node": "^6.2.0",
		"typescript": "^3.1.6",
		"typescript-eslint-parser": "^20.1.1"
	},
	"author": "Nicheal Pham",
	"license": "MIT"
}
